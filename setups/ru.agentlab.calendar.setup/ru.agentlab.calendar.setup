<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:mylyn="http://www.eclipse.org/oomph/setup/mylyn/1.0"
    xmlns:projects="http://www.eclipse.org/oomph/setup/projects/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/mylyn/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Mylyn.ecore http://www.eclipse.org/oomph/setup/projects/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Projects.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore"
    name="calendar"
    label="GoogleCalendar">
  <setupTask
      xsi:type="git:GitCloneTask"
      id="ru.agentlab.calendar.git.clone"
      remoteURI="agentlab/ru.agentlab.calendar">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>github.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>${scope.project.label} Github repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="com.google.calendar.api.samples.git.clone"
      remoteURI="google/google-api-java-client-samples"
      pushURI=""
      checkoutBranch="master">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>github.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>${scope.project.label} Github repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup.targlets:TargletTask">
    <targlet
        name="${scope.project.label}"
        activeRepositoryList="${eclipse.target.platform}">
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/TargetDefinitionGenerator">
        <detail
            key="location">
          <value>${ru.agentlab.calendar.git.clone.location/releng/ru.agentlab.calendar.releng/ru.agentlab.calendar.releng.target}</value>
        </detail>
        <detail
            key="includeAllPlatforms">
          <value>false</value>
        </detail>
        <detail
            key="includeSource">
          <value>true</value>
        </detail>
        <detail
            key="generateVersions">
          <value>false</value>
        </detail>
        <detail
            key="generateImplicitUnits">
          <value>true</value>
        </detail>
      </annotation>
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/PomArtifactUpdater">
        <detail
            key="skipArtifactIDs">
          <value>false</value>
        </detail>
        <detail
            key="skipVersions">
          <value>false</value>
        </detail>
      </annotation>
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/PomModulesUpdater">
        <detail
            key="location">
          <value>${ru.agentlab.calendar.git.clone.location/pom.xml}</value>
        </detail>
      </annotation>
      <requirement
          name="ch.qos.logback.slf4j"
          versionRange="[1.0.7,1.1.0)"/>
      <requirement
          name="ch.qos.logback.classic"
          versionRange="[1.0.7,1.1.0)"/>
      <requirement
          name="ch.qos.logback.core"
          versionRange="[1.0.7,1.1.0)"/>
      <requirement
          name="org.slf4j.api"
          versionRange="[1.7.2,1.8.0)"/>
      <requirement
          name="org.slf4j.jcl"
          versionRange="[1.7.2,1.8.0)"/>
      <requirement
          name="org.slf4j.ext"
          versionRange="[1.7.2,1.8.0)"/>
      <requirement
          name="org.slf4j.jul"
          versionRange="[1.7.2,1.8.0)"/>
      <requirement
          name="org.slf4j.log4j"
          versionRange="[1.7.2,1.8.0)"/>
      <requirement
          name="com.google.inject.multibindings"
          versionRange="[3.0.0,3.1.0)"/>
      <requirement
          name="com.google.inject.assistedinject"
          versionRange="[3.0.0,3.1.0)"/>
      <requirement
          name="org.apache.commons.codec"
          versionRange="[1.4.0,2.0.0)"/>
      <requirement
          name="org.apache.commons.io"
          versionRange="[2.2.0,2.3.0)"/>
      <requirement
          name="org.apache.commons.collections"
          versionRange="[3.2.0,3.3.0)"/>
      <requirement
          name="org.hamcrest.core"
          versionRange="[1.3.0,2.0.0)"/>
      <requirement
          name="org.hamcrest.generator"
          versionRange="[1.3.0,2.0.0)"/>
      <requirement
          name="org.hamcrest.integration"
          versionRange="[1.3.0,2.0.0)"/>
      <requirement
          name="org.hamcrest.library"
          versionRange="[1.3.0,2.0.0)"/>
      <requirement
          name="org.mockito.feature.feature.group"
          versionRange="[1.9.5,2.0.0)"/>
      <requirement
          name="com.google.jsr305.feature.feature.group"
          versionRange="[1.0.0,1.1.0)"/>
      <requirement
          name="com.google.guava.feature.feature.group"
          versionRange="[18.0.0,18.1.0)"/>
      <requirement
          name="org.eclipse.e4.rcp.feature.group"/>
      <requirement
          name="org.eclipse.equinox.sdk.feature.group"/>
      <requirement
          name="org.eclipse.swt.dummyfragments.feature.group"/>
      <requirement
          name="com.codeaffine.osgi.test.util.feature.feature.group"/>
      <requirement
          name="org.junit.feature.feature.group"
          versionRange="[4.12.0,5.0.0)"/>
      <requirement
          name="org.eclipse.fx.core.feature.feature.group"/>
      <requirement
          name="org.eclipse.fx.runtime.feature.feature.group"/>
      <requirement
          name="org.eclipse.fx.target.rcp4.feature.feature.group"/>
      <requirement
          name="org.eclipse.fx.target.feature.feature.group"/>
      <requirement
          name="org.eclipse.fx.slf4j.target.feature.feature.group"/>
      <requirement
          name="org.eclipse.e4.tools.bundle.spy.feature.feature.group"/>
      <requirement
          name="org.eclipse.e4.tools.context.spy.feature.feature.group"/>
      <requirement
          name="org.eclipse.e4.tools.css.spy.feature.feature.group"/>
      <requirement
          name="org.eclipse.e4.tools.event.spy.feature.feature.group"/>
      <requirement
          name="org.eclipse.e4.tools.preference.spy.feature.feature.group"/>
      <requirement
          name="org.eclipse.e4.tools.model.spy.feature.feature.group"/>
      <repositoryList
          name="Neon">
        <repository
            url="http://download.eclipse.org/tools/orbit/downloads/drops/R20160520211859/repository"/>
        <repository
            url="http://agentlab.ru/eclipse/update/com.google.jsr305/1.0.0.201502020347"/>
        <repository
            url="http://agentlab.ru/eclipse/update/com.google.guava/18.0.0.201502021635"/>
        <repository
            url="http://fappel.github.io/xiliary"/>
        <repository
            url="http://agentlab.ru/eclipse/update/org.junit/4.12.0/"/>
        <repository
            url="http://agentlab.ru/eclipse/update/org.mockito/1.10.19/"/>
        <repository
            url="http://download.eclipse.org/e4/snapshots/org.eclipse.e4.tools/latest"/>
        <repository
            url="http://download.eclipse.org/efxclipse/runtime-nightly/site"/>
        <repository
            url="http://download.eclipse.org/modeling/tmf/xtext/updates/composite/releases/"/>
        <repository
            url="http://agentlab.ru/p2/orbit/missing-swt-fragments/"/>
      </repositoryList>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsImportTask">
    <sourceLocator
        rootFolder="${com.google.calendar.api.samples.git.clone.location/calendar-cmdline-sample}"
        locateNestedProjects="true"/>
    <sourceLocator
        rootFolder="${ru.agentlab.calendar.git.clone.location}"
        locateNestedProjects="true"/>
    <description></description>
  </setupTask>
  <setupTask
      xsi:type="mylyn:MylynQueriesTask"
      connectorKind="github"
      repositoryURL="https://github.com/agentlab/ru.agentlab.calendar"
      userID="${github.user.id}"
      password="${github.user.password}">
    <query summary="${scope.project.label} Мои открытые задачи">
      <attribute
          key="state"
          value="open::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
    <query summary="${scope.project.label} Мои закрытые задачи"
        url="">
      <attribute
          key="state"
          value="closed::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsBuildTask"
      id="InitialBuild"
      refresh="true">
    <description>Initial Build</description>
  </setupTask>
  <stream name="master"/>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='ru.agentlab']"/>
  <description>IDE Setup for Calendar Project</description>
</setup:Project>
